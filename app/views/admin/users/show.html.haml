%h1= "#{@user.name} - <#{@user.email}>"
%p
  %strong Sign in count:
  = @user.sign_in_count
%p
  %strong Current Sign in at:
  = @user.current_sign_in_at || 'N/A'
%p
  %strong Current Sign in ip:
  = @user.current_sign_in_ip || 'Never'
%p
  %strong Last Sign in at:
  = @user.last_sign_in_at || 'N/A'
%p
  %strong Last Sign in ip:
  = @user.last_sign_in_ip || 'Never'
%a= link_to 'Back', admin_users_path